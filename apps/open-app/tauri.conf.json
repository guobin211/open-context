{
  "$schema": "https://schema.tauri.app/config/2",
  "app": {
    "macOSPrivateApi": true,
    "security": {
      "capabilities": ["default"],
      "csp": null
    },
    "windows": [
      {
        "height": 600,
        "hiddenTitle": true,
        "minHeight": 600,
        "minWidth": 800,
        "title": "",
        "titleBarStyle": "Overlay",
        "transparent": true,
        "width": 800
      }
    ]
  },
  "build": {
    "beforeBuildCommand": "pnpm build:web",
    "beforeDevCommand": "echo '      start vite dev server http://localhost:1420'",
    "devUrl": "http://localhost:1420",
    "frontendDist": "./../open-web/dist"
  },
  "bundle": {
    "active": true,
    "createUpdaterArtifacts": true,
    "icon": ["icons/32x32.png", "icons/128x128.png", "icons/128x128@2x.png", "icons/icon.icns", "icons/icon.ico"],
    "targets": "all"
  },
  "identifier": "com.guobin.open-context",
  "plugins": {
    "updater": {
      "endpoints": ["https://github.com/user/open-context/releases/latest/download/latest.json"],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDA5MEUwOEM0MkY0NEU0MTYKUldRVzVFUXZ4QWdPQ2I1YUNUTlRHRUZSaXRzclVWUXo3NVJySFZ0dFlRbm4rNHZKN1ZPcHl5eWQK"
    }
  },
  "productName": "open-context",
  "version": "0.1.0"
}
