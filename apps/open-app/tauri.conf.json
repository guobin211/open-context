{
  "$schema": "https://schema.tauri.app/config/2",
  "app": {
    "macOSPrivateApi": true,
    "security": {
      "capabilities": ["default"],
      "csp": {
        "default-src": "'self' asset: http: https: ipc: blob: data:"
      },
      "dangerousDisableAssetCspModification": true,
      "assetProtocol": {
        "enable": true,
        "scope": {
          "allow": ["**/*"],
          "requireLiteralLeadingDot": false
        }
      }
    },
    "windows": [
      {
        "label": "main",
        "title": "",
        "url": "/launcher",
        "height": 590,
        "width": 680,
        "decorations": false,
        "minimizable": true,
        "maximizable": true,
        "skipTaskbar": true,
        "resizable": false,
        "acceptFirstMouse": true,
        "shadow": true,
        "transparent": true,
        "fullscreen": false,
        "center": false,
        "visible": false,
        "windowEffects": {
          "effects": [],
          "radius": 0
        },
        "visibleOnAllWorkspaces": true,
        "alwaysOnTop": true
      },
      {
        "label": "app-settings",
        "title": "Settings",
        "url": "/settings",
        "width": 1000,
        "minWidth": 1000,
        "height": 700,
        "minHeight": 700,
        "center": true,
        "transparent": true,
        "maximizable": false,
        "skipTaskbar": false,
        "dragDropEnabled": false,
        "hiddenTitle": true,
        "visible": false,
        "windowEffects": {
          "effects": ["sidebar"],
          "state": "active"
        }
      },
      {
        "label": "app-playground",
        "title": "Playground",
        "url": "/playground",
        "height": 600,
        "hiddenTitle": true,
        "minHeight": 600,
        "minWidth": 800,
        "titleBarStyle": "Overlay",
        "transparent": true,
        "width": 800
      },
      {
        "label": "app-workspace",
        "title": "Workspace",
        "url": "/workspace",
        "height": 600,
        "hiddenTitle": true,
        "minHeight": 600,
        "minWidth": 800,
        "titleBarStyle": "Overlay",
        "transparent": true,
        "width": 800
      }
    ]
  },
  "build": {
    "beforeBuildCommand": "pnpm build:web",
    "beforeDevCommand": "echo '      start vite dev server http://localhost:1420'",
    "devUrl": "http://localhost:1420",
    "frontendDist": "./../open-web/dist"
  },
  "bundle": {
    "active": true,
    "createUpdaterArtifacts": true,
    "icon": ["icons/32x32.png", "icons/128x128.png", "icons/128x128@2x.png", "icons/icon.icns", "icons/icon.ico"],
    "targets": "all"
  },
  "identifier": "com.guobin.open-context",
  "plugins": {
    "updater": {
      "endpoints": ["https://github.com/user/open-context/releases/latest/download/latest.json"],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDA5MEUwOEM0MkY0NEU0MTYKUldRVzVFUXZ4QWdPQ2I1YUNUTlRHRUZSaXRzclVWUXo3NVJySFZ0dFlRbm4rNHZKN1ZPcHl5eWQK"
    }
  },
  "productName": "open-context",
  "version": "0.1.0"
}
